@model IEnumerable<League>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Leagues</h1>

<div class="row mb-3 text-center">
    <div class="col-4 themed-grid-col">League</div>
    <div class="col-4 themed-grid-col">Season</div>
</div>
    @foreach (var item in Model)
    {
        <div class="row mb-3 text-center">
            <div class="col-4 themed-grid-col">@item.LeagueName</div>
            <div class="col-4 themed-grid-col">@item.LegueSeason</div>
        </div>
    }